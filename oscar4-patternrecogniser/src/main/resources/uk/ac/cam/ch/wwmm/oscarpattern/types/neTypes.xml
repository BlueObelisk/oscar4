<neTypes>
	<type name="CM">
		<desc>(Chemical) A chemical compound/class of compounds</desc>
		<subtype name="EXACT"/>
		<subtype name="CLASS"/>
		<subtype name="PART"/>
		<subtype name="SURFACE"/>
		<subtype name="SPECIES"/>
		<subtype name="POLYMER"/>
		<subtype name="OTHER"/>
	</type>
	<type name="RN">
		<desc>(Reaction) A chemical verb or nominalisation thereof, eg. demethylation</desc>
		<subtype name="REACT"/>
		<subtype name="MOVE"/>
		<subtype name="DESC"/>
		<subtype name="OTHER"/>
	</type>
	<type name="CJ">
		<desc>(Chemical adjective) A chemical in adjective form eg. benzylic</desc>
		<subtype name="EXACT"/>
		<subtype name="CLASS"/>
		<subtype name="PART"/>
		<subtype name="ACID"/>
		<subtype name="SOLUTION"/>
		<subtype name="RECEPTOR"/>
		<subtype name="OTHER"/>
	</type>
	<type name="CPR">
		<desc>(Chemical prefix) eg 1,2- in 1,2-transposition</desc>
		<subtype name="OTHER"/>
	</type>
	<type name="ASE">
		<desc>Single word enzyme names-from-chemicals eg. peroxidase</desc>
		<subtype name="ACTIVITY"/>
		<subtype name="PROTEIN"/>
		<subtype name="OTHER"/>
	</type>
	<type name="PRW">
		<desc>Potential Reaction Word</desc>
		<subtype name="REACT"/>
		<subtype name="DESC"/>
		<subtype name="OTHER"/>
	</type>
	<type name="ONT">
		<desc>A term that doesn't fit into any other category</desc>
		<subtype name="OTHER"/>
	</type>
</neTypes>